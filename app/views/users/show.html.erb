<table class="table">
  <tr>
    <h1>Voici la liste des évenements que tu as créé <%= current_user.name %>:<h1>
    
    <th>Description</th>
    <th>Date & Time</th>
    <th>Place</th>
    <th>Attendees</th>
  </tr>
  
  <% current_user.created_events.each do |event| %>
    <tr>
      <td><%= event.description %></td>
      <td><%= event.date%></td>
      <td><%= event.place%></td>
      <td><%= event.attendees.count %></td>
    </tr>
  <% end %>
